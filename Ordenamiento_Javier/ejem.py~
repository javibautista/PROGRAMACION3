def insertion_sort(lista):
    indice = 1
    #itt = 1
    #ind = 0
    while indice < len(lista):
        #print('i = %d'%(i))
        ins_min = lista[indice]
        elem_izq = indice - 1
        it = 0
        while elem_izq >= 0 and (ins_min < lista[elem_izq]):
            lista[elem_izq+1] = lista[elem_izq]
            lista[elem_izq] = ins_min
            #lista[pos_min] = tmp
            print(lista)
            elem_izq -= 1
            it += 1
            print('while_interno %s'%(it))
            print('elemento %s itercambio %s lugar/es'%(ins_min, it))
        indice += 1
        #ind += 1
        #print('while_externo %s'%(itt))
        #itt += 1
    return lista
if __name__ == '__main__':
    #lista = [6,7,8,9,3,4,5,2,1]
    lista = [1, 2, 3, 4, -5, 6, 7, 8, 0]
    print(insertion_sort(lista))
